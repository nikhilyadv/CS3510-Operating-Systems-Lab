int main()
{
decl
    str r;
    int temp,fd,counter;
enddecl
begin
    fd=exposcall("Open","root");
    counter = 0;
    while (counter < 60) do
    	temp=exposcall("Read",fd, r);
    	if (r != "-1") then
			temp=exposcall("Write",-2,r);
		endif;
		temp=exposcall("Seek",fd,7);
		counter = counter + 2;
	endwhile;
    return 0;
end
}
